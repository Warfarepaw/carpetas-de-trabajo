package main;

public class Test {
	
	public static void main(String [] args) 
	{
		changeCase("This is Interview");
	}
	
	public static String changeCase(String input) 
	{
		
		char[] a = input.toCharArray();
		StringBuilder cCase = null;
		for(int i = 0; i < 10 ; i++) 
		{
			int b = (int) a[i];
			if(b > 65 || b < 91) 
			{
				b = b + 32;
				a[i] = (char) b;
				System.out.println(a[i]);
			}
			
			if(b > 97 || b < 123) 
			{
				b = b - 32;
				a[i] = (char) b;
				System.out.println(a[i]);
			}
			
			
			
			
		}
		
		
		return "";
	}

}
