package main;

public class Test {
	
	public static void main(String [] args) 
	{
		changeCase("ABCD");
	}
	
	public static String changeCase(String input) 
	{
		input.toLowerCase();
		input.toUpperCase();
		
		System.out.println(input.toLowerCase() + "   /n " +input.toUpperCase());
		return "";
	}

}
